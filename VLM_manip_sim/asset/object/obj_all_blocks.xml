            <geom mesh="obj_bowl_collision_3" class="bowl_collision"/>
            <geom mesh="obj_bowl_collision_4" class="bowl_collision"/>
            <geom mesh="obj_bowl_collision_5" class="bowl_collision"/>
            <geom mesh="obj_bowl_collision_6" class="bowl_collision"/>
            <geom mesh="obj_bowl_collision_7" class="bowl_collision"/>
